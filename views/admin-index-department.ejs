<h1>Departments</h1>

<table border="1">
    <tr>
        <th>Department</th>
        <th>Visibility</th>
        <th colspan="2">Actions</th>
    </tr>
    <% departments.forEach(department => { %>
        <tr>
            <td><%= department.department %></td>
            <td><%= department.visibility %></td>
            <td><a href="/admin/edit-department/<%= department.id %>">Edit</a></td>
            <td><button>Delete</button></td>
        </tr>
    <% }); %>
</table>
