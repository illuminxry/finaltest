<h1>All subjects</h1>
<a href="/admin/dashboard">Admin Dashboard</a>
<br>
<br>
<button><a href="/admin/create-subject">Create Subject</a></button>
<table border="1">
    <tr>
        <th>Subject ID</th>
        <th>Subject Name</th>
        <th>Section</th>
        <th>Teacher ID</th>
        <th>Assigned teacher</th>
        <th colspan="3">Action</th>
    </tr>
    <% data.subjects.forEach((subject) => { %>
        <tr>
            <td><%= subject.subjectid %></td>
            <td><%= subject.subjectname %></td>
            <td><%= subject.sectionname %></td>
            <td><%= subject.teacherid %></td>
            <% data.teachers.forEach((teacher) => { %>
                <% if (teacher.id === subject.teacherid) { %>
                    <td><%= teacher.firstname + ' ' + teacher.middlename + ' ' + teacher.lastname %></td>
                <% } %>
            <% }); %>
            <td></td>
            <td></td>
            <td></td>
        </tr>
    <% }); %>
    
</table>
